 
<%= render 'header_leads' %>
<div class="row">
  <div class="col s12 m12 l12">
    <div class="container ">
      <p id="notice"><%= notice %></p>
      <h4>Leads Management</h4>
      <%= link_to 'New Lead', new_lead_path, class:"waves-effect waves-light btn bk-yellow home" %>
      <a class="waves-effect waves-light btn bk-yellow home" href="">Download Excel </a>
      <table>
        <thead>
          <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Company</th>
            <th>Created Date</th>
            <th>Required Information</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @lead.each do |lead| %>
          <tr>
            <td><%= lead.name %></td>
            <td><%= lead.email %></td>
            <td><%= lead.phone %></td>
            <td><%= lead.company %></td>
            <td><%= lead.created_at %></td>
            <td>
              <ul>Comming Soon!
              </ul>
            </td>
            <td><%= link_to 'Show', lead %></td>
            <td><%# link_to 'Edit', edit_lead_path(lead) %></td>
            <td><%# link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

<%= render 'leads/layouts/footer_leads' %>
